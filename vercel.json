{
  "version": 2,
  "functions": {
    "app.py": {
      "runtime": "python3.10",
      "memory": 1024
    }
  },
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python@3.10"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "methods": ["POST"],
      "dest": "app.py"
    }
  ]
}
